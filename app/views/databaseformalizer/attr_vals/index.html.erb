<div id="modelPicture"></div>
<h1>Listing attr_vals</h1>

<table>
  <tr>
    <th>Value</th>
    <th>Entity</th>
    <th>Attr def name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @attr_vals.each do |attr_val| %>
  <tr>
    <td><%= attr_val.value %></td>
    <td><%= attr_val.entity %></td>
    <td><%= attr_val.attr_def_name %></td>
    <td><%= link_to 'Show', attr_val %></td>
    <td><%= link_to 'Edit', edit_databaseformalizer_attr_val_path(attr_val) %></td>
    <td><%= link_to 'Destroy', attr_val, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

